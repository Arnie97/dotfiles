[alias]
	aa = add --all
	ap = add --patch
	au = add --update
	bi = bisect
	bs = bisect
	bl = blame
	br = branch
	bw = browse
	ce = clean
	cf = config --edit --global
	ch = checkout
	cl = clone --recursive
	cm = commit --amend
	ca = commit --amend --reset-author
	cc = !GIT_COMMITTER_DATE=`git log --pretty=%aD HEAD^..` git commit --amend
	cw = follow
	co = commit
	cp = cherry-pick
	cr = create
	de = tell
	di = diff
	ds = diff --stat
	dn = diff --no-index
	dt = difftool
	dw = diff --word-diff
	fb = filter-branch
	fe = fetch
	fp = format-patch
	gg = log --graph --date='format:%y-%m-%d' --pretty=custom
	gl = log --graph --date='format:%y-%m-%d' --pretty=custom --all
	gr = grep
	ig = ignore
	lg = log --stat --summary --pretty=fuller
	ls = ls-files
	lt = ls-tree --long --abbrev
	mb = merge-base
	me = merge --allow-unrelated-histories
	mt = mergetool
	pf = push --force-with-lease
	pl = pull --ff-only
	pr = pull --rebase
	pu = push
	pv = !TZ=UTC git show --quiet --pretty='v0.0.0-%cd-%h' --date='format:%Y%m%d%H%M%S' --abbrev=12
	rb = rebase
	rd = range-diff
	re = reset
	rh = reset --hard
	rs = reset --soft
	rf = reflog
	ri = rebase --interactive
	rm = rm --cached
	ro = remote
	rp = rev-parse
	rq = pull-request
	rr = rerere
	rt = rebase --committer-date-is-author-date
	rv = !git push origin HEAD:refs/for/${1:-$(git rev-parse --abbrev-ref HEAD)}%${2}
	sh = show
	sc = sparse-checkout
	sp = sparse-checkout
	sl = status --long --show-stash
	ss = status --short
	st = stash
	su = submodule
	sw = switch
	tf = tag --force
	us = update-server-info
[blame]
	coloring = highlightRecent
[color "blame"]
	highlightRecent = reset, 6 month ago, cyan, 3 month ago, blue, 1 month ago, green, 1 week ago, yellow, 1 day ago, red
[core]
	abbrev = 7
	autoCRLF = false
	quotePath = false
	sparseCheckout = true
[diff]
	mnemonicPrefix = true
	renames = copies
	wsErrorHighlight = all
[feature]
	experimental = true
[fetch]
	parallel = 0
[gc]
	auto = 128
[grep]
	lineNumber = true
[gui]
	encoding = utf-8
[http]
	lowSpeedLimit = 20480
	lowSpeedTime = 120
[hub]
	protocol = https
[log]
	follow = true
	mailmap = true
[mailmap]
	file = ~/.mailmap
[merge]
	directoryRenames = true
	guiTool = bc
	tool = vimdiff
[pretty]
	custom = %C(red bold)%h%C(reset) %C(cyan)%cd%C(yellow bold)%d%C(reset) %s %C(cyan)(%C(green)%aN, %C(cyan)%cr)%C(reset)
[protocol]
	version = 2
[push]
	default = matching
[rebase]
	autoStash = true
	instructionFormat = [%aN] %s (%cr)% d
[rerere]
	autoUpdate = true
	enabled = true
[tag]
	sort = version:refname
[user]
	email = arnie97@gmail.com
	name = Arnie97

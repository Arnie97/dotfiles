	name = Arnie97
[core]
	autocrlf = false
	quotepath = false
[diff]
	tool = bc
[merge]
	tool = bc
[credential]
	helper = store
[alias]
	aa = add --all
	au = add --update
	bi = bisect
	bs = bisect
	bl = blame
	br = branch
	bw = browse
	ce = clean
	cf = config --edit --global
	ch = checkout
	cl = clone
	cm = commit --amend
	co = commit
	cp = cherry-pick
	cr = create
	de = tell
	di = diff
	ds = diff --stat
	dt = difftool
	dw = diff --word-diff
	fb = filter-branch
	fe = fetch
	gl = log --graph --all --date-order --abbrev=4 --date='format:%y-%m-%d' --pretty=format:'%C(red bold)%h%C(reset) %C(cyan)%cd%C(yellow bold)%d%C(reset) %s %C(green)<%an> %C(blue bold)(%ar)%C(reset)'
	gr = grep
	ig = ignore
	in = init
	ls = ls-files
	me = merge --allow-unrelated-histories
	mt = mergetool
	pa = format-patch
	pf = push --force
	pl = pull
	pr = pull --rebase
	pu = push
	rb = rebase
	re = reset
	rh = reset --hard
	rs = reset --soft
	rf = reflog
	ri = rebase --interactive
	rl = rev-list
	rm = rm --cached
	ro = remote
	rp = rev-parse
	rq = pull-request
	sh = show
	ss = status --short
	st = status
	su = submodule
	tf = tag --force
	tm = filter-branch --force --env-filter 'export GIT_COMMITTER_DATE="$GIT_AUTHOR_DATE"'
	ui = update-server-info
	us = update-server-info
[hub]
	protocol = https
[push]
	default = matching
[color]
	ui = auto

[user]
	email = arnie97@gmail.com
	name = Arnie97
[core]
	autocrlf = false
	quotepath = false
[credential]
	helper = store
[alias]
	aa = add --all
	au = add --update
	bl = blame
	br = branch
	bw = browse
	ce = clean
	cf = config --edit --global
	ch = checkout
	cl = clone
	cm = commit --amend
	co = commit
	cp = cherry-pick
	cr = create
	de = describe --tags
	di = diff
	ds = diff --stat
	dt = difftool
	dw = diff --word-diff
	fb = filter-branch
	fe = fetch
	gl = log --graph --all --date-order --date=short --abbrev=4 --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cd) %C(bold blue)<%an>%Creset'
	gr = grep
	ig = !git-ignore
	in = init
	me = merge
	mt = mergetool
	pa = format-patch
	pf = push --force
	pl = pull
	pr = pull --rebase
	pu = push
	rb = rebase
	re = reset
	rf = reflog
	ri = rebase --interactive
	rl = rev-list
	rm = rm --cached
	ro = remote
	rp = rev-parse
	rq = pull-request
	sh = show
	ss = status --short
	st = status
	su = submodule
	tf = tag --force
	tm = filter-branch --force --env-filter 'export GIT_COMMITTER_DATE="$GIT_AUTHOR_DATE"'
[hub]
	protocol = https
[push]
	default = matching
[color]
	ui = auto
